{
  "expo": {
    "name": "eVikno",
    "slug": "eVikno",
    "owner": "vitaliypritula",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Logo.png",
    "scheme": "com.evikno.app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.evikno.app",
      "edgeToEdgeEnabled": true,
      "jsEngine": "hermes"  
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.evikno.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "jsEngine": "jsc"         
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Logo.png"
    },
    "router": {
      "unstable_settings": {
        "initialRouteName": "(tabs)"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "75cec0a0-c72b-47f0-86dc-1c26174e9b15"
      }
    }
  }
}
